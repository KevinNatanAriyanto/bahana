    "react-native-code-push": "^5.7.0",
    "react-native-collapsible": "^1.5.2",
    "react-native-config": "^0.12.0",
    "react-native-device-info": "^6.0.4",
    "react-native-document-picker": "^3.2.4",
    "react-native-fbsdk": "1.1.1",
    "react-native-firebase": "^5.5.6",
    "react-native-flash-message": "^0.1.16",
    "react-native-geolocation-service": "^5.3.0-beta.1",
    "react-native-gesture-handler": "^1.4.1",
    "react-native-google-signin": "^2.1.0",
    "react-native-image-crop-picker": "0.25.3",
    "react-native-image-picker": "^1.1.0",
    "react-native-keychain": "3.1.3",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-localize": "^1.0.0",

{
  "name": "thechef",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "node ./android/fixAndroid/android-release-fix.js; node ./bin/postInstall; bash replace-pods.sh; bash scripts/fix-lib.sh;",
    "build": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/ && rm -rf ./android/app/src/main/res/drawable-* && rm -rf ./android/app/src/main/res/raw/",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/thechef/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'",
    "release": "yarn build && cd ./android && ENVFILE=.env ./gradlew bundleRelease",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint index.js app --fix --ext .js,.ts,.tsx",
    "compile": "tsc --noEmit -p . --pretty",
    "format": "npm-run-all format:*",
    "format:js": "prettier --write {.,**}/*.js",
    "format:json": "prettier --write {.,**}/*.json",
    "format:md": "prettier --write {.,**}/*.md",
    "format:ts": "prettier --write {.,**}/*.{ts,tsx}",
    "hack:types-react-navigation": "rimraf node_modules/@types/react-navigation/node_modules/@types",
    "hack:types-react-native": "rimraf node_modules/@types/react-native/node_modules/@types",
    "hack:types-react-test-renderer": "rimraf node_modules/@types/react-test-renderer/node_modules/@types",
    "patch": "patch-package",
    "prepare": "npm-run-all patch hack:*",
    "storybook": "storybook start -p 9001 --skip-packager",
    "adb": "adb reverse tcp:9090 tcp:9090 && adb reverse tcp:3000 tcp:3000 && adb reverse tcp:9001 tcp:9001 && adb reverse tcp:8081 tcp:8081",
    "android-install": "adb install android/app/outputs/apk/release/app-release.apk",
    "android-install-debug": "adb install android/app/build/outputs/apk/debug/app-debug.apk",
    "android-clean": "cd android && gradlew clean && cd ..",
    "android-raw": "cd android && cd app && cd src && cd main && cd res && cd raw && del /f/q/s *.* && cd .. && cd .. && cd .. && cd .. && cd .. && cd ..",
    "android-run": "SET ENVFILE=.env.production && npm run android-clean && adb devices && react-native run-android",
    "android-start": "SET ENVFILE=.env.production && npm run android-clean && ipconfig && react-native start",
    "osx-android-bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "osx-build-apk-staging": "cd android; ./gradlew clean; yarn build; ENVFILE=.env ./gradlew bundleRelease; cd ..; bundletool build-apks --bundle=android/app/build/outputs/bundle/release/app.aab --output=android/app/build/outputs/bundle/release/app.apks --ks=android/eyesimple.keystore --ks-key-alias=eyesimple --ks-pass=pass:eyesimple",
    "osx-build-apk-prod": "cd android; ./gradlew clean; yarn build; ENVFILE=.env.production ./gradlew bundleRelease; cd ..; bundletool build-apks --bundle=android/app/build/outputs/bundle/release/app.aab --output=android/app/build/outputs/bundle/release/app.apks --ks=android/eyesimple.keystore --ks-key-alias=eyesimple --ks-pass=pass:eyesimple",
    "osx-generate-apk-staging": "cd android; ./gradlew clean; cd ..; npm run osx-android-bundle; rm -rf android/app/src/main/res/raw; ENVFILE=.env react-native run-android --variant=release;",
    "osx-generate-apk-production": "cd android; ./gradlew clean; cd ..; npm run osx-android-bundle; rm -rf android/app/src/main/res/raw; ENVFILE=.env.production react-native run-android --variant=release;",
    "win-generate-apk-production": "npm run android-clean && npm run osx-android-bundle && npm run android-raw && SET ENVFILE=.env.production && react-native run-android --variant=release",
    "android-codepush-app-staging": "ENVFILE=.env code-push release-react bahana android -d Staging --targetBinaryVersion '*'",
    "ios-codepush-app-staging": "ENVFILE=.env code-push release-react bahana ios -d Staging --targetBinaryVersion '*'",
    "android-codepush-app-production": "ENVFILE=.env.production code-push release-react bahana android -d Production --targetBinaryVersion '*'"
  },
  "dependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.9.6",
    "@mauron85/react-native-background-geolocation": "0.5.6",
    "@react-native-community/async-storage": "^1.5.1",
    "@react-native-community/geolocation": "^2.0.2",
    "@react-native-community/netinfo": "^5.9.7",
    "@supersami/rn-foreground-service": "^1.1.1",

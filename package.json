    "react-native-search-box": "^0.0.19",
    "react-native-section-list-get-item-layout": "^2.2.3",
    "react-native-slider": "^0.11.0",
    "react-native-snap-carousel": "^3.8.1",
    "react-native-spinkit": "^1.5.0",
    "react-native-splash-screen": "3.1.1",
    "react-native-tab-view": "^2.10.0",
    "react-native-thumbnail-video": "^0.1.2",
    "react-native-vector-icons": "6.6.0",
    "react-native-webview": "^7.4.3",
    "react-native-wifi-reborn": "^4.2.0",
    "react-native-youtube": "^2.0.0",
    "react-navigation": "3.11.1",
    "reactotron-mst": "^3.1.1",
    "reactotron-react-native": "^4.0.0-beta.1",
    "realm": "^6.1.2",
    "recyclerlistview": "^2.0.12",
    "rn-bottom-drawer": "^1.4.3",
    "rn-sliding-up-panel": "^2.3.3",
